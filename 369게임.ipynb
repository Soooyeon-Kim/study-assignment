    "n = input(\"시작 수와 마지막 수를 입력하세요\").split()\n",
    "m = map(int, n)        \n",
    "a, b = m               \n",
    "count = 0\n",
    "for i in range(a, b+1):\n",
    "    j = str(i)\n",
    "    for k in j:\n",
    "        if k in ('3','6','9'):\n",
    "            # print(\"짝\", end='')\n",
    "            count += 1\n",
    "    # print()\n",
    "print(count, \"번\")"
